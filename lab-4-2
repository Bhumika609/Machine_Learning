import numpy as np
import pandas as pd
from sklearn.model_selection import train_test_split
from sklearn.neighbors import KNeighborsRegressor
from sklearn.metrics import mean_squared_error,r2_score,mean_absolute_percentage_error
file_path="Lab Session Data.xlsx"
data=pd.read_excel(file_path,sheet_name="IRCTC Stock Price")
features=["Open","High","Low"]
X=data[features].values
Y=data["Price"].values
X_train,X_test,Y_train,Y_test=train_test_split(X,Y,test_size=0.3,random_state=42)
neigh=KNeighborsRegressor(n_neighbors=3)
neigh.fit(X_train,Y_train)
y_pred=neigh.predict(X_test)
print("The mean square error is",mean_squared_error(Y_test,y_pred))
print("The root mean Squared error is",np.sqrt(mean_squared_error(Y_test,y_pred)))
print("The r2 score is",r2_score(Y_test,y_pred))
print("The mean_absolute error percentage is",mean_absolute_percentage_error(Y_test,y_pred))